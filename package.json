{
  "name": "fanbook-api-node-sdk",
  "description": "Fanbook OpenAPI JavaScript / Node.js SDK",
  "version": "0.4.4",
  "packageManager": "pnpm@8.3.1",
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "files": [
    "/lib",
    "/es",
    "/README.md",
    "/CHANGELOG.md",
    "/LICENSE"
  ],
  "scripts": {
    "build": "sh scripts/build.sh",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "lint": "eslint .",
    "doc-gen": "ts-node scripts/doc-gen.ts",
    "docs-to-interface": "ts-node scripts/docs-to-interface.ts",
    "generate-api-docs": "ts-node scripts/generate-api-docs.ts",
    "docs:dev": "vitepress dev docs --port 8771",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "devDependencies": {
    "@inquirer/prompts": "^3.2.0",
    "@microsoft/api-documenter": "^7.23.11",
    "@microsoft/tsdoc": "^0.14.2",
    "@microsoft/tsdoc-config": "^0.16.2",
    "@swc/core": "^1.3.96",
    "@swc/helpers": "^0.5.3",
    "@swc/jest": "^0.2.29",
    "@types/jest": "^29.5.7",
    "@types/json-bigint": "^1.0.3",
    "@types/node": "^18.18.8",
    "@types/qs": "^6.9.9",
    "@types/uuid": "^9.0.6",
    "@types/ws": "^8.5.8",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "cross-env": "^7.0.3",
    "eslint-plugin-tsdoc": "^0.2.17",
    "fast-xml-parser": "^4.3.2",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "ts-node": "^10.9.1",
    "tsconfig": "^7.0.0",
    "typedoc": "^0.24.8",
    "typedoc-plugin-markdown": "^3.17.0",
    "typescript": "^5.2.2",
    "vitepress": "1.0.0-rc.24",
    "vue": "^3.3.7"
  },
  "repository": {
    "url": "https://github.com/DevOpen-Club/api-node-sdk"
