    "mitt": "^3.0.1",
    "uuid": "^10.0.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.9.0",
    "@babel/preset-typescript": "^7.24.1",
    "@inquirer/prompts": "^5.3.8",
    "@shikijs/vitepress-twoslash": "^1.2.0",
    "@swc/core": "^1.4.8",
    "@swc/helpers": "^0.5.7",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.12",
    "@types/json-bigint": "^1.0.4",
