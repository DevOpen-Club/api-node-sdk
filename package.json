  "packageManager": "pnpm@9.7.0",
  "description": "Fanbook OpenAPI JavaScript / Node.js SDK",
  "author": "DevOpen Club",
  "license": "MIT",
  "homepage": "https://fanbook-api-sdk.js.org/",
