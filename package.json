  "dependencies": {
    "axios": "^1.6.0",
    "js-base64": "^3.7.5",
    "json-bigint": "^1.0.0",
    "mitt": "^3.0.1",
    "qs": "^6.11.2",
    "uuid": "^9.0.1",
    "ws": "^8.14.2"
  }
