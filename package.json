    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "fanbook-api-node-sdk": "link:.",
    "fast-xml-parser": "^4.3.2",
    "jest": "^29.7.0",
    "tsx": "^4.7.1",
    "typedoc": "^0.26.5",
    "typedoc-plugin-markdown": "4.2.5",
    "typescript": "^5.4.3",
